{#
/**
 * @file simplemeta.html.twig
 * Default theme implementation to present SimpleMeta data.
 *
 * This template is used when viewing SimpleMeta pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_simplemeta()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('simplemeta') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
